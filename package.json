{
  "name": "guide-to-pdf",
  "version": "1.0.0",
  "private": true,
  "main": "./build/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "start-renderer": "cross-env BROWSER=none yarn start",
    "start-main": "wait-on http://localhost:3000 && electron .",
    "compile-main": "tsc src/electron/*.ts --outDir build --esModuleInterop true",
    "dev": "yarn compile-main && concurrently -n 'renderer,main' 'npm:start-renderer' 'npm:start-main'",
    "dist": "electron-builder build",
    "predist": "yarn build && yarn compile-main"
  },
  "dependencies": {
    "@types/eslint": "^8.4.1",
    "@types/node": "^17.0.23",
    "@types/puppeteer": "^5.4.5",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.24",
    "antd": "^4.19.3",
    "electron-is-dev": "^2.0.0",
    "normalize-url": "^4.5.1",
    "pdf-merger-js": "^3.4.0",
    "puppeteer": "^13.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "styled-components": "^5.3.5",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron": "^17.2.0",
    "electron-builder": "^22.14.13",
    "wait-on": "^6.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
